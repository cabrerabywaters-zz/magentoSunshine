<config>
	<translate>
		<modules>
			<Magoch_Externlinks>
				<files>
					<default>Magoch_Externlinks.csv</default>
				</files>
			</Magoch_Externlinks>
		</modules>
	</translate>
	<acl>
		<resources>
			<admin>
				<children>
					<system>
						<children>
							<config>
								<children>
									<externlinks translate="title" module="externlinks">
										<title>Extrenal Links</title>
										<sort_order>70</sort_order>
									</externlinks>
								</children>
							</config>
						</children>
					</system>
				</children>
			</admin>
		</resources>
	</acl>
</config>